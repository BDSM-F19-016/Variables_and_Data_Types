# Variable 'a' stores the integer value 12
a = 12
print(a)  # Print the value stored in variable 'a'
print("Int:", type(a))  # Print the type of variable 'a'

# Variable 'b' stores the string "Ali"
b = "Ali"
print(b)  # Print the value stored in variable 'b'
print(type(b))  # Print the type of variable 'b'

# Variable 'c' stores the floating-point number 5.4
c = 5.4
print(c)  # Print the value stored in variable 'c'
print(type(c))  # Print the type of variable 'c'

# Variable 'l' stores a list containing various data types
l = ["Ali", 5, 4.6, 2000]
print(l)  # Print the value stored in variable 'l'
print(type(l))  # Print the type of variable 'l'

# Variable 't' stores a tuple containing integer values
t = (1, 2, 3, 4, 5)
print(t)  # Print the value stored in variable 't'
print(type(t))  # Print the type of variable 't'

# Variable 'B' stores the boolean value False
B = False
print(B)  # Print the value stored in variable 'B'
print(type(B))  # Print the type of variable 'B'
